{
    "manifest_version": 3,
    "name": "Email This Page",
    "version": "2.0",
    "description": "A utility extension to email web pages",
    "permissions": [
        "contextMenus", "activeTab"
      ],

      "icons": {
          "16": "logo.png",
          "128": "logo128.png"
      },

      "action": {
        "default_icon": {                   
          "16": "logo.png"   
        }
        },        
    "background": {
      "service_worker": "background.js"
    }
}
